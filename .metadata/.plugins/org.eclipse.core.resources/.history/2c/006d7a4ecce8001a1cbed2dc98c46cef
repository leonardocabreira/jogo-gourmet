package model;


public class No {
    
    private Generic objetoGenerico;
    private No direita;
    private No esquerda;

    public Generic getObjetoGenerico() {
        return objetoGenerico;
    }
       

    public No(Generic objetoGenerico, No direita, No esquerda) {
		
		this.objetoGenerico = objetoGenerico;
		this.direita = direita;
		this.esquerda = esquerda;
	}


	public void setDescricao(Generic objetoGenerico) {
        this.objetoGenerico = objetoGenerico;
    }

    public No getDireita() {
        return direita;
    }

    public void setDireita(No direita) {
        this.direita = direita;
    }

    public No getEsquerda() {
        return esquerda;
    }

    public void setEsquerda(No esquerda) {
        this.esquerda = esquerda;
    }
    
}
