package util;

import model.No;

public class FactoryGame {

	
	public static No factoryGame() {
		
		 		No lasanha = new No();
		        lasanha.setDescricao("Lasanha");
		        lasanha.setSim(null);
		        lasanha.setNao(null);

		        No bolo = new No();
		        bolo.setDescricao("Bolo de Chocolate");
		        bolo.setSim(null);
		        bolo.setNao(null);

		        No massa = new No();
		        massa.setDescricao("Massa");
		        massa.setSim(lasanha);
		        massa.setNao(bolo);

		        return massa;
		    
	}
}
